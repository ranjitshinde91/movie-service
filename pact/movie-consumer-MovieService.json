{
  "provider": {
    "name": "MovieService"
  },
  "consumer": {
    "name": "movie-consumer"
  },
  "interactions": [
    {
      "description": "GET REQUEST",
      "request": {
        "method": "GET",
        "path": "/movies/DDLJ"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "releaseDate": "2000-01-31",
          "name": "DDLJ"
        },
        "matchingRules": {
          "body": {
            "$.name": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.releaseDate": {
              "matchers": [
                {
                  "match": "date",
                  "date": "yyyy-MM-dd"
                }
              ],
              "combine": "AND"
            }
          }
        },
        "generators": {
          "body": {
            "$.releaseDate": {
              "type": "Date",
              "format": "yyyy-MM-dd"
            }
          }
        }
      },
      "providerStates": [
        {
          "name": "Test Get"
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "3.6.7"
    }
  }
}